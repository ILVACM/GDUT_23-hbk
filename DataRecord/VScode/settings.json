{
    "security.workspace.trust.untrustedFiles": "open",
    "editor.fontFamily": "JetBrains Mono, '楷体', JetBrains Mono",
    "workbench.settings.applyToAllProfiles": [
        "editor.fontFamily",
        "markdown.preview.fontFamily",
        "markdown.styles",
        "terminal.explorerKind",
        "terminal.integrated.defaultLocation",
        "terminal.integrated.defaultProfile.windows",
        "terminal.integrated.profiles.windows",
        "terminal.integrated.rightClickBehavior"
    ],
    "markdown.preview.fontFamily": "JetBrains Mono, '楷体', JetBrains Mono",
    "markdown-preview-enhanced.codeBlockTheme": "dark.css",
    "markdown-preview-enhanced.previewTheme": "github-dark.css",
    "markdown.styles": [
        "D:\\TOOLS\\VSCode\\config\\Markdown_PDF\\font.css"
    ],
    "markdown-pdf.styles": [
        "D:\\TOOLS\\VSCode\\config\\Markdown_PDF\\font.css"
    ],
    "editor.minimap.enabled": false,
    "git.confirmSync": false,
    "git.autofetch": true,
    "files.associations": {
        "*.txt": "plaintext"
    },
    "git.enableSmartCommit": true,
    "explorer.confirmDragAndDrop": false,
    "cmake.showOptionsMovedNotification": false,
    "cmake.configureOnOpen": true,
    "workbench.colorTheme": "Visual Studio 2017 Dark - C++",
    "terminal.explorerKind": "both",
    "terminal.integrated.defaultLocation": "editor",
    "terminal.integrated.defaultProfile.windows": "Command Prompt",
    "terminal.integrated.profiles.windows": {
        "PowerShell": {
            "source": "PowerShell",
            "icon": "terminal-powershell"
        },
        "Command Prompt": {
            "path": [
                "${env:windir}\\Sysnative\\cmd.exe",
                "${env:windir}\\System32\\cmd.exe"
            ],
            "args": [
                "/K",
                "D:\\Python\\anaconda3\\Scripts\\activate.bat D:\\Python\\anaconda3"
            ],
            "icon": "terminal-cmd"
        },
        "Git Bash": {
            "source": "Git Bash"
        }
    },
}